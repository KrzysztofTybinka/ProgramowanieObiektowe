<Window x:Class="Projekt.AddCategoryWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt"
        mc:Ignorable="d"
        Title="AddCategoryWin" Height="200" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="49*"/>
            <RowDefinition Height="135*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="1" Margin="10,7,10,10">
            <TextBox Height="40" x:Name="box" FontSize="15" VerticalContentAlignment="Center"/>
            <Label/>
            <Button Background="#FFB3E5F1" Content="Dodaj" Margin="100,0,100,0" x:Name="Add" Click="Add_Click"/>
            <Label x:Name="message" FontSize="12"/>
        </StackPanel>
    </Grid>
</Window>
